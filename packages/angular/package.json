{
    "name": "@storybook-extras/angular",
    "version": "0.0.62",
    "description": "Storybook addon to add extra configurations for Angular applications.",
    "license": "MIT",
    "keywords": [
        "storybook",
        "addons",
        "extras",
        "angular",
        "services",
        "unit testing",
        "docs",
        "compodoc",
        "wrapper",
        "coverage",
        "test runner",
        "instrument",
        "webpack",
        "source code",
        "storybook addons"
    ],
    "author": {
        "name": "Mostafa Sherif",
        "email": "mosherif87@gmail.com"
    },
    "homepage": "https://github.com/sheriffMoose/storybook-extras/tree/master/packages/devkit#readme",
    "funding": "https://github.com/sheriffMoose/storybook-extras?sponsor=1",
    "repository": {
        "type": "git",
        "url": "https://github.com/sheriffMoose/storybook-extras.git",
        "directory": "packages/devkit"
    },
    "publishConfig": {
        "access": "public",
        "registry": "https://registry.npmjs.org/"
    },
    "storybook": {
        "displayName": "Angular Extras",
        "supportedFrameworks": [
            "angular"
        ],
        "icon": "https://raw.githubusercontent.com/sheriffMoose/storybook-extras/master/logos/angular.svg"
    },
    "engines": {
        "node": ">=16"
    },
    "main": "dist/index.js",
    "module": "dist/index.js",
    "types": "dist/index.d.ts",
    "files": [
        "dist",
        "stories",
        "package.json",
        "README.md",
        "CHANGELOG.md",
        "LICENSE",
        "*.js",
        "*.d.ts"
    ],
    "scripts": {
        "*************** DEV ***************": "",
        "build:cjs": "tsc",
        "build:esm": "",
        "build:ts": "",
        "*************** BUILD ***************": "",
        "prebuild": "rimraf ./dist",
        "build": "concurrently \"yarn build:cjs\" \"yarn build:esm\" \"yarn build:ts\"",
        "build:watch": "concurrently \"yarn build:cjs --watch\" \"yarn build:esm --watch\" \"yarn build:ts --watch\""
    },
    "dependencies": {
        "@storybook-extras/devkit": "workspace:^",
        "node-polyfill-webpack-plugin": "^2.0.1",
        "webpack-plugin-istanbul": "^1.0.3"
    },
    "devDependencies": {
        "@angular/core": "latest",
        "@storybook/addon-essentials": "next",
        "@storybook/angular": "next",
        "@storybook/builder-vite": "^7.0.0-beta.41",
        "concurrently": "^7.6.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "rimraf": "^3.0.2",
        "storybook": "next",
        "tslib": "^2.5.0",
        "typescript": "4.9.5",
        "vite": "^4.0.0"
    },
    "peerDependencies": {
        "@storybook/addons": "*",
        "@storybook/api": "*",
        "@storybook/components": "*",
        "@storybook/core-events": "*",
        "@storybook/theming": "*",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    },
    "gitHead": "bc5ab559b9e7fe9c199d19a87d3970ccd0758a03"
}
